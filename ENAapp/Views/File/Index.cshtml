@model ENAapp.Models.Compte

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<form asp-action="SetImage" enctype="multipart/form-data">
    <div class="row">
        <div class="form-group">
            <label asp-for="Imgsrc" class="control-label"></label>
            <input type="file" asp-for="Imgsrc" class="form-control" />
            <span asp-validation-for="Imgsrc" class="text-danger"></span>
        </div>
    </div>
    <div class="row">
        <div class="col-md-8" style="padding-top:10px;">
            <input type="submit" value="Import" class="btn btn-default" />
        </div>
    </div>
    <br />
</form>
